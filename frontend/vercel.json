
{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.{js,jsx,ts,tsx}",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites":  [
    {"source": "/(.*)", "destination": "/"}
  ]
}
